<?xml version="1.0"?>

<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone"
    purge="False">

    <configlet
        title="ejournal Subscription Settings"
        action_id="ejsubscriptions"
